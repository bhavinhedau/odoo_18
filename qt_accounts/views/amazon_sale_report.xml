            </list>
        </field>
    </record>

    <record id="amazon_invoice_action" model="ir.actions.act_window">
        <field name="name">Amazon Report</field>
        <field name="res_model">amazon.invoice.report</field>
        <field name="view_mode">list,form</field>
    </record>
        <menuitem id="amazon_report_id" name="Amazon Report" action="amazon_invoice_action"/>

