<odoo>
    <record id="seq_machinery_progress" model="ir.sequence">
        <field name="name">Machinery Progress</field>
        <field name="code">machinery.progress</field>
        <field name="prefix">MP</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_project_master" model="ir.sequence">
        <field name="name">Project Master</field>
        <field name="code">project.master</field>
        <field name="prefix">PM</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_plant_master" model="ir.sequence">
        <field name="name">Plant Master</field>
        <field name="code">plant.master</field>
        <field name="prefix">P</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_equipment_master" model="ir.sequence">
        <field name="name">Equipment Master</field>
        <field name="code">equipment.master</field>
        <field name="prefix">EQUI</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
